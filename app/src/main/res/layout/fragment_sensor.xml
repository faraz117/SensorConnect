<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.electronicjunkies.sensorconnect.sensorconnect.SensorFragment">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/accXValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="22dp"
        android:layout_marginTop="210dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/accZValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/accYValue"
        android:layout_alignBottom="@+id/accYValue"
        android:layout_marginStart="44dp"
        android:layout_toEndOf="@+id/accYValue"
        android:text="TextView" />

    <TextView
        android:id="@+id/accYValue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginStart="47dp"
        android:layout_alignBaseline="@+id/accXValue"
        android:layout_alignBottom="@+id/accXValue"
        android:layout_toEndOf="@+id/accXValue" />
</RelativeLayout>
